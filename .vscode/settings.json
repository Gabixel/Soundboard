{
	"less.compile": {
		"out": "${workspaceFolder}/src/css/",
		"compress": true
	},
	"files.exclude": {
		"**/node_modules": true
	},
	"terminal.integrated.cwd": "${workspaceFolder}",
	"terminal.integrated.profiles.windows": {
		"Soundboard's TypeScript compiler (Command Prompt)": {
			"path": "cmd.exe",
			"args": ["/K", "npm run tsc-all-w"],
			"cwd": "${workspaceFolder}"
		},
		"Soundboard's TypeScript compiler (PowerShell)": {
			"path": "powershell.exe",
			"args": ["-NoLogo", "-NoExit", "-Command", "npm run tsc-all-w"],
			"cwd": "${workspaceFolder}"
		}
	},
	"terminal.integrated.defaultProfile.windows": "Command Prompt"
}
