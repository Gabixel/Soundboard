{
	"name": "soundboard",
	"version": "1.0.0",
	"description": "A soundboard for your friends!",
	"productName": "Soundboard",
	"author": "Gabixel",
	"license": "MIT",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"tsc-all": "concurrently \"npm:tsc-app\" \"npm:tsc-src\"",
		"tsc-app": "cd ./app && tsc -b -w",
		"tsc-src": "cd ./src/ts && tsc -b -w"
	},
	"keywords": [
		"soundboard",
		"audio",
		"voice"
	],
	"dependencies": {
		"discord-rpc": "^4.0.1",
		"dotenv": "^16.0.1",
		"electron": "^19.0.6",
		"jquery": "^3.6.0",
		"npm": "^8.11.0"
	},
	"devDependencies": {
		"@types/jquery": "^3.5.14",
		"concurrently": "^7.6.0"
	},
	"optionalDependencies": {}
}
