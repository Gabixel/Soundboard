{
	"name": "soundboard",
	"version": "1.0.0",
	"description": "A soundboard for your friends!",
	"productName": "Soundboard",
	"author": "Gabixel",
	"license": "MIT",
	"main": "./app/main.js",
	"scripts": {
		"debug": "electron .",
		"tsc-all-w": "concurrently \"npm:tsc-app-w\" \"npm:tsc-src-w\"",
		"tsc-app-w": "cd ./app && tsc -b -w",
		"tsc-src-w": "cd ./src/ts && tsc -b -w",
		"tsc-all": "concurrently \"npm:tsc-app\" \"npm:tsc-src\"",
		"tsc-app": "cd ./app && tsc -b",
		"tsc-src": "cd ./src/ts && tsc -b",
		"start": "electron-forge start",
		"package": "electron-forge package",
		"make": "electron-forge make"
	},
	"keywords": [
		"soundboard",
		"audio-player",
		"audio",
		"voice",
		"player",
		"track",
		"sound"
	],
	"dependencies": {
		"discord-rpc": "^4.0.1",
		"dotenv": "^16.0.1",
		"electron-squirrel-startup": "^1.0.0",
		"jquery": "^3.6.0",
		"npm": "^10.2.1"
	},
	"devDependencies": {
		"@electron-forge/cli": "^6.0.4",
		"@electron-forge/maker-deb": "^6.0.4",
		"@electron-forge/maker-rpm": "^6.0.4",
		"@electron-forge/maker-squirrel": "^6.0.4",
		"@electron-forge/maker-zip": "^6.0.4",
		"@types/jquery": "^3.5.14",
		"concurrently": "^7.6.0",
		"electron": "^25.0.1"
	}
}
